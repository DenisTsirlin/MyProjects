<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cream"
    android:orientation="vertical"
    android:weightSum="10"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView100"
        android:layout_width="153dp"
        android:layout_height="0dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="95dp"
        android:layout_marginBottom="115dp"
        android:fontFamily="@font/seaweed_script"
        android:text="@string/menu"
        android:textColor="@color/black"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/frameLayout"
        app:layout_constraintTop_toTopOf="parent" />


    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="254dp"
        android:layout_height="0dp"
        android:layout_marginTop="95dp"
        android:layout_marginBottom="382dp"
        android:background="@color/brownGray"
        android:rotation="-0"
        android:rotationX="0"
        android:rotationY="0"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/imgGuessPage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="16dp"
            android:background="@color/brownGray"
            android:src="@drawable/question_mark">

        </ImageButton>

        <TextView
            android:id="@+id/textGuess"
            android:layout_width="171dp"
            android:layout_height="34dp"
            android:layout_gravity="bottom"
            android:layout_marginStart="47dp"
            android:fontFamily="@font/akaya_telivigala"
            android:rotationX="21"
            android:text="Guess The Number"
            android:textColor="@color/black"
            android:textSize="20sp">

        </TextView>


    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="254dp"
        android:layout_height="0dp"
        android:layout_marginBottom="22dp"
        android:background="@color/brownGray"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView100">

        <ImageButton
            android:id="@+id/imgCameraPage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="16dp"
            android:background="@color/brownGray"
            android:src="@drawable/camera">

        </ImageButton>

        <TextView
            android:id="@+id/textPicOrVid"
            android:layout_width="171dp"
            android:layout_height="34dp"
            android:layout_gravity="bottom"
            android:layout_marginStart="70dp"
            android:fontFamily="@font/akaya_telivigala"
            android:rotationX="30"
            android:text="Take a Pic/Vid"
            android:textColor="@color/black"
            android:textSize="20sp">

        </TextView>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="254dp"
        android:layout_height="0dp"
        android:layout_marginBottom="30dp"
        android:background="@color/brownGray"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout2">

        <ImageButton

            android:id="@+id/imgCalcPage"
            android:layout_width="wrap_content"
            android:layout_height="57dp"
            android:layout_gravity="center"
            android:layout_marginBottom="16dp"
            android:background="@color/brownGray"
            android:src="@drawable/calculator">

        </ImageButton>

        <TextView
            android:id="@+id/textCalc"
            android:layout_width="171dp"
            android:layout_height="34dp"
            android:layout_gravity="bottom"
            android:layout_marginStart="82dp"
            android:fontFamily="@font/akaya_telivigala"
            android:rotationX="32"
            android:text="Calculator"
            android:textColor="@color/black"
            android:textSize="20sp">

        </TextView>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="254dp"
        android:layout_height="0dp"
        android:layout_marginBottom="26dp"
        android:background="@color/brownGray"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout3">

        <ImageButton
            android:id="@+id/imgGamePage"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="16dp"
            android:background="@color/brownGray"
            android:src="@drawable/game">

        </ImageButton>

        <TextView
            android:id="@+id/textGame"
            android:layout_width="171dp"
            android:layout_height="34dp"
            android:layout_gravity="bottom"
            android:layout_marginStart="87dp"
            android:fontFamily="@font/akaya_telivigala"
            android:rotationX="32"
            android:text="My Game"
            android:textColor="@color/black"
            android:textSize="20sp">

        </TextView>


    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout5"
        android:layout_width="254dp"
        android:layout_height="0dp"
        android:layout_marginBottom="62dp"
        android:background="@color/brownGray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout">

        <ImageButton
            android:id="@+id/imgMapsPage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="16dp"
            android:background="@color/brownGray"
            android:src="@drawable/location">

        </ImageButton>

        <TextView
            android:id="@+id/textLocation"
            android:layout_width="199dp"
            android:layout_height="34dp"
            android:layout_gravity="bottom"
            android:layout_marginStart="107dp"
            android:fontFamily="@font/akaya_telivigala"
            android:rotationX="32"
            android:text="Map"
            android:textColor="@color/black"
            android:textSize="20sp">

        </TextView>

    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>